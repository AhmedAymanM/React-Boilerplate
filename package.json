{
  "name": "app-name",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "build": " cross-env NODE_ENV=production webpack -p --env.env=prod",
    "build:watch": "yarn build --watch",
    "dev": "cross-env webpack-dev-server --env.env=dev",
    "format": "prettier --write --single-quote --print-width=80 --parser=flow --tab-width=2 \"**/*.js\"",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run format --",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/helpers/enzyme.setup.js",
    "moduleNameMapper": {
      "\\.(css|scss|svg|gif)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "antd": "^3.5.2",
    "autoprefixer": "8.5.0",
    "babel-runtime": "^6.26.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "lodash.debounce": "^4.0.8",
    "postcss-flexbugs-fixes": "3.3.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-hot-loader": "^4.1.2",
    "react-redux": "5.0.7",
    "redux": "^4.0.0",
    "redux-mock-store": "1.5.1",
    "redux-thunk": "2.2.0",
    "unfetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-import": "1.7.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "1.1.11",
    "cross-env": "5.1.5",
    "css-loader": "0.28.11",
    "enzyme": "^3.3.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react": "1.1.7",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.8.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "3.2.0",
    "husky": "^0.14.3",
    "jest": "22.4.3",
    "jest-serializer-enzyme": "1.0.0",
    "lint-staged": "^7.0.5",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "4.9.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-loader": "2.1.5",
    "prettier": "1.12.1",
    "progress-bar-webpack-plugin": "1.11.0",
    "react-test-renderer": "16.3.2",
    "resource-hints-webpack-plugin": "^0.0.2",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "2.12.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.3",
    "webpack-hot-middleware": "2.22.2",
    "webpack-merge": "4.1.2"
  }
}
