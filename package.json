{
  "name": "app-name",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "build": " cross-env NODE_ENV=production webpack -p --env.env=prod",
    "build:watch": "yarn build --watch",
    "dev": "cross-env webpack-dev-server --env.env=dev",
    "format": "prettier --write --single-quote --print-width=80 --parser=flow --tab-width=2 \"**/*.js\"",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run format --",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/helpers/enzyme.setup.js",
    "moduleNameMapper": {
      "\\.(css|scss|svg|gif)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@svgr/webpack": "^4.1.0",
    "antd": "^3.10.1",
    "autoprefixer": "9.2.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "lodash.debounce": "^4.0.8",
    "postcss-flexbugs-fixes": "4.1.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-mock-store": "1.5.3",
    "redux-thunk": "2.3.0",
    "unfetch": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-import": "1.7.0",
    "babel-plugin-lodash": "^3.3.2",
    "clean-webpack-plugin": "0.1.19",
    "compression-webpack-plugin": "2.0.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "enzyme": "^3.7.0",
    "eslint": "5.7.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-config-react": "1.1.7",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "2.0.0",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "3.2.0",
    "husky": "^1.1.2",
    "jest": "23.6.0",
    "jest-serializer-enzyme": "1.0.0",
    "lint-staged": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "4.9.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "1.14.3",
    "progress-bar-webpack-plugin": "1.11.0",
    "react-test-renderer": "16.5.2",
    "resource-hints-webpack-plugin": "^0.0.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "^1.2.1",
    "url-loader": "1.1.2",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "3.0.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.9",
    "webpack-hot-middleware": "2.24.3",
    "webpack-merge": "4.1.4"
  }
}
